                "-mn","DeepSD"]  
        },
        {
            "name": "THST",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "justMyCode": false,
            "args": [ "-mts" ,"{'stochastic':False,'stochastic_f_pass':1,'distr_type':'None','discrete_continuous':False,'precip_threshold':0.5,'var_model_type':'Deterministic','location':'region_grid','deformable_conv':True, 'model_version':'1'}", 
                "-mn","THST","-dd","Data/Rain_Data_Nov19", "-bs","2"]  
        },
